<!DOCTYPE html>
<html>
  <head>
    <title>ShippedProject</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>


    <nav>
    <% if (user_signed_in?) %>
    	<%= link_to "Sign Out" , {:action=>"destroy", :controller=>"devise/sessions"}, method: :delete %>

    	<% else%>
    	<h3 class="pull-right"><%= link_to "Sign In", new_user_session_path %></h3>
    	<h3 class="pull-right"><%= link_to "Sign Up", new_user_registration_path %></h3>
    <% end %>
</nav>



    <%= yield %>
  </body>
</html>
